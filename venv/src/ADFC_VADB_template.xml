<Event id="${eventItemId}">
    <creationTime>${publishDate}</creationTime>
    <languages multiple="true">
        <Language id="1">
            <name>de</name>
        </Language>
    </languages>
    <title>
        <I18n id="12627993">
            <de>
                <![CDATA[${titel}]]>
            </de>
        </I18n>
    </title>
    <shortDescription>
        <I18n id="12627992">
            <de>
                <![CDATA[${kurz}]]>
            </de>
        </I18n>
    </shortDescription>
    <link>
        <I18n id="12627988">
            <de>
                <![CDATA[${frontendLink}]]>
            </de>
        </I18n>
    </link>
    <categories>
        <ExpandCategories/>
    </categories>
    <pricing>
        <ExpandPricing/>
    </pricing>
    <eventDates>
        <SpecificEventDate>
            <date>${date}</date>
            <startTime>${time}</startTime>
            <duration>${duration}</duration>
        </SpecificEventDate>
    </eventDates>
    <media>
        <EventImage>
            <pooledMedium>
                <PooledEventMedium url="${imageUrl}">
                    <copyright>
                        <I18n>
                            <de>
                                <![CDATA[${copyright}]]>
                            </de>
                        </I18n>
                    </copyright>
                </PooledEventMedium>
            </pooledMedium>
            <imageType>
                <!-- note: depending on customer project -->
                <ImageType id="1"/>
            </imageType>
        </EventImage>
    </media>
    <location>
        <AddressPoi id="${addressPoi}"/>
    </location>
    <contributor>
        <AddressPoi id="6137"/>
    </contributor>
</Event>
